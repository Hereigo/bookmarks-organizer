{
  "manifest_version": 2,
  "name": "__MSG_extension_name__",
  "version": "1.0",
  "description": "__MSG_extension_description__",
  "icons": {
    "48": "images/icon-48.png",
    "96": "images/icon-96.png"
  },
  "developer": {
    "name": "SÃ¶ren Hentzschel",
    "url": "https://www.soeren-hentzschel.at"
  },
  "background": {
    "scripts": ["js/status.js", "js/background.js"]
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "64": "images/icon-64.png"
    },
    "default_title": "__MSG_button_check_bookmarks__"
  },
  "omnibox": {
    "keyword" : "bookmarks"
  },
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "<all_urls>",
    "bookmarks",
    "storage",
    "tabs"
  ],
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "bookmarkchecker@agenedia.com",
      "strict_min_version": "51.0"
    }
  }
}
